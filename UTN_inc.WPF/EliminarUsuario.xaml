<Window x:Class="UTN_inc.WPF.EliminarUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UTN_inc.WPF"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Icon="Assets/pepito.ico"
        Title="EliminarUsuario" Height="180" Width="400">

    <Grid Background="Wheat">

        <TextBlock HorizontalAlignment="Left" Margin="30,40,0,0" TextWrapping="Wrap" Text="Usuario ID:" VerticalAlignment="Top" Width="100"/>

        <TextBox x:Name="textIdEliminarUsuario" HorizontalAlignment="Left" Margin="100,40,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <Image Source="/pepito.jpg" HorizontalAlignment="Left" Height="98" Margin="250,40,0,0" VerticalAlignment="Top" Width="90"/>

        <Button Content="Eliminar" HorizontalAlignment="Left" Width="100" Margin="100,80,0,0" VerticalAlignment="Top" Click="Button_Eliminar"/>
        <Button Content="Atras" HorizontalAlignment="Left" Width="100" Margin="100,110,0,0" VerticalAlignment="Top" Click="Button_Salir"/>

    </Grid>

</Window>
