<Window x:Class="UTN_inc.WPF.Eliminar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UTN_inc.WPF"
        mc:Ignorable="d"
        Icon="Assets/pepito.ico"
        WindowStartupLocation="CenterScreen"
        Title="Eliminar" Height="200" Width="400">

    <Grid Background="Wheat">

        <TextBlock HorizontalAlignment="Left" Margin="25,40,0,0" TextWrapping="Wrap" Text="Producto ID:" VerticalAlignment="Top" Width="100"/>
        <TextBox x:Name="textProductoIdEliminar" HorizontalAlignment="Left" Margin="100,40,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Image Source="/pepito.jpg" HorizontalAlignment="Left" Height="98" Margin="250,40,0,0" VerticalAlignment="Top" Width="90"/>
        <Button Content="Eliminar" HorizontalAlignment="Left" Width="100" Margin="100,75,0,0" VerticalAlignment="Top" Click="Button_Eliminar"/>
        <Button Content="Atras" HorizontalAlignment="Left" Width="100" Margin="100,105,0,0" VerticalAlignment="Top" Click="Button_Atras"/>

    </Grid>

</Window>
